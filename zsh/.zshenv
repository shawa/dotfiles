# Where to store the history
HISTFILE=~/.zsh_history

# How much history to load on startup
HISTSIZE=1000

# How much history to store in the file
SAVEHIST=10000000

# Make really slow nvm more lazy
NVM_NO_USE=true

# Where zplug is install
ZPLUG_HOME=/usr/local/opt/zplug

# How much lag before exiting insert mode
PATH="$PATH:$HOME/.local/src/elixir-ls:$HOME/.local/bin"
CDPATH=".:$HOME"
KEYTIMEOUT=1

PYOPENCL_CTX='0:1,0'
KEYTIMEOUT=0
